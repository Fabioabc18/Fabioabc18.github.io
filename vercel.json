{
    "version": 2,
    "routes": [
      { "src": "/accounts/(.*)", "dest": "/accounts/views.py" },
      { "src": "/anuncios/(.*)", "dest": "/anuncios/views.py" },
      { "src": "/categorias/(.*)", "dest": "/categorias/views.py" },
      { "src": "/contactos/(.*)", "dest": "/contactos/views.py" },
      { "src": "/home/(.*)", "dest": "/home/views.py" },
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/templates/registration/(.*)", "dest": "/templates/registration/$1" }
    ],
    "builds": [
      { "src": "accounts/views.py", "use": "@now/python" },
      { "src": "anuncios/views.py", "use": "@now/python" },
      { "src": "categorias/views.py", "use": "@now/python" },
      { "src": "contactos/views.py", "use": "@now/python" },
      { "src": "home/views.py", "use": "@now/python" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/OXL/$1" }
    ]
  }
  